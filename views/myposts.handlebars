<main>
    {{#each posts}}
    <div class="post">
        <div class="post-title">
            <h2><a href="/post/{{id}}">{{title}}</h2></a>
            <div>
                <p>Posted by {{username}} on {{formate_date createdAt}}</p>
            </div>
        </div>
        <div class="post-content">
            <p class="content">{{content}}</p>
        </div>
    </div>
    {{/each}}

    
</main>
<h1>mypost</h1>
<main>
    {{#each posts}}
    <a href="/dash/edit/{{id}}">
    <div class="post">
        <div class="post-title">
            <h2>{{title}}</h2>
        </div>
        <div class="post-content">
            <p class="content">{{content}}</p>
        </div>
    </div>
    </a>
    {{/each}}
    <div id="create-btn-div">
        <a href=""><button id="create-btn">Create Blog Post</button></a>
    </div>
</main>